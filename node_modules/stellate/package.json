    "@graphql-tools/json-file-loader": "^7.3.4",
    "@graphql-tools/load": "^7.5.2",
    "@graphql-tools/url-loader": "^7.8.0",
    "@graphql-tools/utils": "^8.10.0",
    "@types/is-ci": "^3.0.0",
    "@types/node": "^18.17.5",
    "@types/prompts": "^2.0.11",
    "arg": "^5.0.0",
    "chalk": "^4.1.0",
    "debug": "^4.3.1",
    "graphql": "^16.8.0",
    "graphql-yoga": "^4.0.0",
    "make-dir": "^3.1.0",
    "node-fetch": "2.6.7",
    "open": "^7.4.0",
    "ora": "5.4.1",
    "prompts": "^2.4.2",
    "strip-indent": "3.0.0",
    "table": "^6.0.7",
    "terminal-link": "^2.1.1",
    "timeago.js": "^4.0.2",
    "tsup": "6.7.0",
    "tsx": "^3.12.1",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0",
    "ws": "8.11.0",
    "yaml": "^1.10.2",
    "@gcdn/configuration": "1.39.2"
  },
  "bin": {
    "stellate": "dist/bin.js",
    "graphcdn": "dist/bin.js"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./rate-limiting": {
      "types": "./dist/rate-limiting.d.ts",
      "import": "./dist/rate-limiting.mjs",
      "require": "./dist/rate-limiting.js"
    },
    "./apollo-server": {
      "types": "./dist/apollo-server.d.ts",
      "import": "./dist/apollo-server.mjs",
      "require": "./dist/apollo-server.js"
    },
    "./graphql-yoga": {
      "types": "./dist/graphql-yoga.d.ts",
      "import": "./dist/graphql-yoga.mjs",
      "require": "./dist/graphql-yoga.js"
    },
    "./envelop": {
      "types": "./dist/envelop.d.ts",
      "import": "./dist/envelop.mjs",
      "require": "./dist/envelop.js"
    }
  },
  "typesVersions": {
    "*": {
      "rate-limiting": [
        "dist/rate-limiting.d.ts"
      ],
      "apollo-server": [
        "dist/apollo-server.d.ts"
      ],
      "graphql-yoga": [
        "dist/graphql-yoga.d.ts"
      ],
      "envelop": [
        "dist/envelop.d.ts"
      ]
    }
  },
  "files": [
    "apollo-server.js",
    "apollo-server.mjs",
    "graphql-yoga.js",
    "graphql-yoga.mjs",
    "envelop.js",
    "envelop.mjs",
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@atomist/yaml-updater": "^1.0.2",
    "@sindresorhus/slugify": "^1.1.0",
    "date-fns": "^3.2.0",
    "esbuild": "^0.17.7",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.6",
    "is-ci": "^3.0.1",
    "lz-string": "1.5.0",
    "random-words": "^1.1.1",
    "zod": "^3.22.4"
  },
  "scripts": {
    "build": "tsup && rm dist/bin.mjs",
    "stellate": "tsx src/bin.ts",
    "test": "FORCE_COLOR=0 vitest run",
    "test:watch": "vitest"
  }
}