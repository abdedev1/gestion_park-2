  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@apollo/server": "^4.10.2",
    "@envelop/core": "^4.0.0",
    "@graphql-codegen/plugin-helpers": "^5.0.1",
